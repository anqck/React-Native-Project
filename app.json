{
  "expo": {
    "name": "Location Notifier",
    "description": "This project is really great.",
    "slug": "Location-Notifier",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
